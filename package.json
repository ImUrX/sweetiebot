{
  "name": "sweetiebot",
  "version": "2.0.0",
  "description": "A discord bot made for your weird needs",
  "main": "bin/src/index.js",
  "repository": "https://github.com/ImUrX/sweetiebot.git",
  "author": "ImUrX <urielfontan2002@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.16"
  },
  "type": "module",
  "scripts": {
    "genSchemas": "npx get-graphql-schema \"https://graphql.anilist.co/\" -j | tee assets/schemas/anilist.json",
    "getFonts": "chmod +x ./getFonts.sh && ./getFonts.sh",
    "updateDump": "node --experimental-json-modules updatedump.js",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "start": "npm run compile && node --experimental-json-modules bin/src/index.js",
    "dev": "npm start | pino-pretty -i pid,hostname -t 'yyyy-mm-dd HH:MM:ss'"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/he": "^1.1.2",
    "@types/node": "^18.7.1",
    "@types/sharp": "^0.30.5",
    "@types/wanakana": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "pino-pretty": "^9.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@discordjs/builders": "^1.2.0",
    "@discordjs/rest": "*",
    "@discordjs/voice": "^0.11.0",
    "@napi-rs/canvas": "^0.1.26",
    "bufferutil": "^4.0.6",
    "canvas": "^2.9.3",
    "chalk": "^5.0.1",
    "cheerio": "^1.0.0-rc.12",
    "common-tags": "^1.8.2",
    "discord-api-types": "^0.37.4",
    "discord.js": "^14.3.0",
    "fake-tag": "^4.0.0",
    "form-data": "^4.0.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "he": "^1.2.0",
    "intl-messageformat": "^10.1.1",
    "knex": "^2.2.0",
    "moment": "^2.29.4",
    "ms": "^3.0.0-canary.1",
    "mysql2": "^2.3.3",
    "node-fetch": "^3.2.10",
    "pino": "^7.11.0",
    "sharp": "^0.30.7",
    "squares-rng": "^2.0.0",
    "ts-toolbelt": "^9.6.0",
    "undici": "^5.9.1",
    "utf-8-validate": "^5.0.9",
    "wanakana": "^5.0.2",
    "zlib-sync": "^0.1.7"
  }
